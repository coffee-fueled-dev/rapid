{
  "name": "@rapid/core",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "import": "./src/client.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts"
    },
    "./routes": {
      "types": "./src/routes/index.ts",
      "import": "./src/routes/index.ts"
    },
    "./query-params": {
      "types": "./src/query-parameters/index.ts",
      "import": "./src/query-parameters/index.ts"
    },
    "./query-params/client": {
      "types": "./src/query-parameters/client.ts",
      "import": "./src/query-parameters/client.ts"
    },
    "./query-params/server": {
      "types": "./src/query-parameters/server.ts",
      "import": "./src/query-parameters/server.ts"
    },
    "./types": {
      "types": "./src/types.ts",
      "import": "./src/types.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=bun --format=esm",
    "build:server": "bun build src/server.ts --outdir=dist --target=bun --format=esm",
    "build:client": "bun build src/client/index.ts --outdir=dist --target=browser --format=esm",
    "dev": "bun --watch src/server.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@rapid/css": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "unocss": "^66.2.0"
  }
}
