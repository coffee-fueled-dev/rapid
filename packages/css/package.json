{
  "name": "@rapid/css",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=bun --format=esm",
    "dev": "bun --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "unocss": "^66.2.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
